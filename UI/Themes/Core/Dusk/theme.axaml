    <!-- Notification Colors -->
    <Color x:Key="NotificationBackgroundPrimary">#2D2D2D</Color>
    <Color x:Key="NotificationBackgroundSecondary">#141414</Color>
    
    <!-- System Colors -->
    <Color x:Key="SystemInfoBackground">#3954b0</Color>
    <Color x:Key="SystemProcessBackground">#886ce4</Color>
    <Color x:Key="SystemWarningBackground">#f0ad4e</Color>
    <Color x:Key="SystemErrorBackground">#d9534f</Color>
    <Color x:Key="SystemSuccessBackground">#5cb85c</Color>
    
