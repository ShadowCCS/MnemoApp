    <Color x:Key="OverlayItemHoverBackgroundColor">#3a3a3a</Color>
    <Color x:Key="OverlayHighlightColor">#4c4c4c</Color>
    
    <!-- Search Bar Colors -->
    <Color x:Key="SearchBarBackground">#2D2D2D</Color>
    <Color x:Key="SearchBarTextColor">#888888</Color>
    
    <!-- Badge Colors -->
    <Color x:Key="BadgeBackground">#2D2D2D</Color>
