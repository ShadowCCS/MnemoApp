    <!-- Widget Colors -->
    <Color x:Key="WidgetBackgroundPrimary">#3A3A3A</Color>
    <Color x:Key="WidgetBackgroundSecondary">#282828</Color>
    
    <!-- Widget Icon Background Colors -->
    <Color x:Key="WidgetIconBackground1">#5B7A9F</Color>
    <Color x:Key="WidgetIconBackground2">#4A6B58</Color>
    <Color x:Key="WidgetIconBackground3">#665170</Color>
    <Color x:Key="WidgetIconBackground4">#6B6A69</Color>
    <Color x:Key="WidgetIconBackground5">#1F1F1F</Color>
    
    <!-- Widget Icon Colors -->
    <Color x:Key="WidgetIconColor1">#87CEFD</Color>
    <Color x:Key="WidgetIconColor2">#92D59E</Color>
