    <Color x:Key="CardIconColor">#AAAAAA</Color>
    <Color x:Key="NotificationIconColor">#848484</Color>
    <Color x:Key="NotificationIconForeground">#262626</Color>
    <Color x:Key="OverlayIconColor">#868686</Color>
    <Color x:Key="LearnCardIconColor">#666666</Color>
    <Color x:Key="ListItemIconColor">#888888</Color>
    <Color x:Key="AccentButtonIcon">#FFFFFF</Color>
    <Color x:Key="GhostButtonIcon">#888888</Color>
    
    <!-- Link Colors -->
    <Color x:Key="Links">#4A9EE7</Color>
    <Color x:Key="LinksHover">#4A9EE7</Color>
    
