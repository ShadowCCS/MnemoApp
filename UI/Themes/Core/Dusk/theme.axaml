    <Color x:Key="WorkspaceBackground">#202020</Color>
    <Color x:Key="OverlayBackground">#2D2D2D</Color>
    <Color x:Key="TopbarBackground">Transparent</Color>
    
    <!-- Text Colors -->
    <Color x:Key="TextPrimary">#DDDDDD</Color>
    <Color x:Key="TextSecondary">#AAAAAA</Color>
    <Color x:Key="TextTertiary">#7B7B7B</Color>
    <Color x:Key="TextFaded">#666666</Color>
    <Color x:Key="TextDisabled">#666666</Color>
    <Color x:Key="NotificationTitleColor">#A2A2A2</Color>
    <Color x:Key="NotificationDescriptionColor">#767676</Color>
    <Color x:Key="BreadcrumbText">#666666</Color>
    <Color x:Key="VersionText">#7B7B7B</Color>
    
    <!-- Icon Colors -->
    <Color x:Key="IconsSidebar">#AAAAAA</Color>
