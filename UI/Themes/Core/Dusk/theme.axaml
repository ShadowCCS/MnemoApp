    <x:Double x:Key="LargeFontSize">24</x:Double>
    <x:Double x:Key="MediumFontSize">20</x:Double>
    <x:Double x:Key="SmallFontSize">14</x:Double>
    <x:Double x:Key="ExtraSmallFontSize">11</x:Double>
    <x:Double x:Key="ExtraExtraSmallFontSize">9</x:Double>

    <!-- Background Colors -->
