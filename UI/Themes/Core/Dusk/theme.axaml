<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Mnemo Dusk Theme -->
    
    <!-- Fonts  Weights-->
    <FontFamily x:Key="MediumFont">avares://MnemoApp/UI/Themes/Fonts/Inter/InterDisplay-Medium.ttf</FontFamily>
    <FontFamily x:Key="BoldFont">avares://MnemoApp/UI/Themes/Fonts/Inter/InterDisplay-Bold.ttf</FontFamily>
    <FontFamily x:Key="SemiBoldFont">avares://MnemoApp/UI/Themes/Fonts/Inter/InterDisplay-SemiBold.ttf</FontFamily>

    <!-- Font Sizes -->
