    <!-- Answer Colors -->
    <Color x:Key="AnswerCorrect">#6A295D</Color>
    <Color x:Key="AnswerWrong">#242D60</Color>
    
    <!-- Logo Colors -->
    <Color x:Key="SidebarLogo">#AAAAAA</Color>
    
