    <SolidColorBrush x:Key="PropertyBrush" Color="{DynamicResource SyntaxProperty}"/>
    
    <!-- Rich Text Highlighting Brushes -->
    <SolidColorBrush x:Key="HighlightedTextBrush" Color="{DynamicResource HighlightedText}"/>

</ResourceDictionary>