<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MnemoApp.Modules.Dashboard"
        xmlns:vm="clr-namespace:MnemoApp.Modules.Dashboard"
        xmlns:services="using:MnemoApp.Core.Services"
        xmlns:i18n="clr-namespace:MnemoApp.UI.Markup"
        mc:Ignorable="d"
        x:Class="MnemoApp.Modules.Dashboard.DashboardView">
    <Grid Margin="10">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                    <TextBlock i18n:Localization.Namespace="Dashboard" i18n:Localization.TextKey="Title" FontSize="{DynamicResource FontSize.Heading4}" Foreground="Black" FontFamily="{DynamicResource Font.Bold}"/>
        <TextBlock i18n:Localization.Namespace="Dashboard" i18n:Localization.TextKey="Description" FontSize="{DynamicResource FontSize.Body.Small}" Foreground="Gray" Margin="0,10,0,0" TextWrapping="Wrap"/>


        </StackPanel>
    </Grid>
</UserControl>