<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MnemoApp.Modules.Learning"
        xmlns:i18n="clr-namespace:MnemoApp.UI.Markup"
        xmlns:vm="clr-namespace:MnemoApp.Modules.Learning"
        mc:Ignorable="d"
        x:Class="MnemoApp.Modules.Learning.FlashcardsView">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                    <TextBlock i18n:Localization.Namespace="Flashcards" i18n:Localization.TextKey="Title" FontSize="{DynamicResource FontSize.Heading4}" Foreground="Black"/>
        <TextBlock i18n:Localization.Namespace="Flashcards" i18n:Localization.TextKey="Coming Soon" FontSize="{DynamicResource FontSize.Body.Small}" Foreground="Gray" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</UserControl> 